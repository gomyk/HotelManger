doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    title Home | HotelNewYork
    link(href='css/bootstrap.min.css', rel='stylesheet')
    link(href='css/font-awesome.min.css', rel='stylesheet')
    link(href='css/animate.min.css', rel='stylesheet')
    link(href='css/lightbox.css', rel='stylesheet')
    link(href='css/main.css', rel='stylesheet')
    link(href='css/responsive.css', rel='stylesheet')
    //if lt IE 9
      script(src='js/html5shiv.js')
      script(src='js/respond.min.js')
    link(rel='shortcut icon', href='images/ico/apple-touch-icon-89-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='225x225', href='images/ico/apple-touch-icon-225-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='179x178', href='images/ico/apple-touch-icon-179-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='113x113', href='images/ico/apple-touch-icon-113-precomposed.png')
    link(rel='apple-touch-icon-precomposed', href='images/ico/apple-touch-icon-89-precomposed.png')
  // /head
  body
    header#header

      .containerCopyright
        .row
          .col-sm-12.overflow
            .social-icons.pull-right(style='margin-right:10%')
              ul.nav.nav-pills
                li
                  p
                    |Sign In ->
                li
                  a(href='/create_account')
                    i.fa.fa-linkedin
      .navbar.navbar-inverse(role='banner')
        .container
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='/')
              h1
                img(src='images/logo.png', alt='logo',style='width:258px;height:80px')
          .collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
              li.active
                a(href='/') Home
              li.dropdown
                a(href='#')
                  | Info
                  i.fa.fa-angle-down
                ul.sub-menu(role='menu')
                  li
                    a(href='/showroom') Rooms
                  li
                    a(href='service.html') Services
                  li
                    a(href='pricing.html') 404 Not found
                  li
                    a(href='contact.html') 404 Not found
                  li
                    a(href='coming-soon.html') Coming Soon

              if grade > 1
                li.dropdown
                    a(href='#')
                      | Companys
                      i.fa.fa-angle-down
                    ul.sub-menu(role='menu')
                      li
                        a(href='/addcompany') Add Company
                      li
                        a(href='/deletecompany') Delete Company
                      li
                        a(href='/updatecompany') Update Company
                      li
                        a(href='/showcompany') Show Company list
                li.dropdown
                  a(href='#')
                    | Stuffs
                    i.fa.fa-angle-down
                  ul.sub-menu(role='menu')
                    li
                      a(href='/addstuff') Add Stuff
                    li
                      a(href='/deletestuff') Delete Stuff
                    li
                      a(href='/updatestuff') Update Stuff
                    li
                      a(href='/showstuff') Show Stuff list
                li.dropdown
                  a(href='#')
                    | Rooms
                    i.fa.fa-angle-down
                  ul.sub-menu(role='menu')
                    li
                      a(href='/addroom') Add Room
                    li
                      a(href='/deleteroom') Delete Room
                    li
                      a(href='/updateroom') Update Room
                    li
                      a(href='/showroomlist') Show Room List
                    li
                      a(href='/showreservation') Reservation Manage

              if grade > 2
                li.dropdown
                    a(href='#')
                      | ADMIN_MENU
                      i.fa.fa-angle-down
                    ul.sub-menu(role='menu')
                      li
                        a(href='/addemployee') Add Employee
                      li
                        a(href='/deleteemployee') Delete Employee
                      li
                        a(href='/updateemployee') Update Employee
                      li
                        a(href='/showemployee') Show Employee List
              if grade == 0
                li.dropdown
                    a(href='#')
                      | My Menu
                      i.fa.fa-angle-down
                    ul.sub-menu(role='menu')
                      li
                        a(href='/showreservation2') My Reservation
                      li
                        a(href='portfoliofour.html') 404 Not found
                      li
                        a(href='portfolioone.html') 404 Not found
                      li
                        a(href='portfoliotwo.html') 404 Not found
                      li
                        a(href='portfoliothree.html') 404 Not found
                      li
                        a(href='portfolio-details.html') 404 Not found
          .search
            form(role='form')
              i.fa.fa-search
              .field-toggle
                input.search-form(type='text', autocomplete='off', placeholder='Search')
    section#action.responsive
                           .vertical-center
                             .container
                               .row
                                 .action.take-tour
                                   .col-sm-7.wow.fadeInLeft(data-wow-duration='500ms', data-wow-delay='300ms')
                                     h1.title Hotel New York Prive Best Services!
                                     p Relax and Comfortable, Great views with coast
                                   .col-sm-5.text-center.wow.fadeInRight(data-wow-duration='500ms', data-wow-delay='300ms')
                                      a.btn.btn-common(href='#') Reservation Now!
                         // /#action
    section#carousel-example-generic.carousel.slide(data-ride='carousel', style='margin-top:30px')
      .carousel-inner(role='listbox')
        -var ob = JSON.parse(data)
        -for(var prop in ob)
          if prop == 0
            .item.active
              center
                  a(href='/reservation' )
                    img(src='#{ob[prop].RESERVNAME}', alt='#{ob[prop].ROOMNUMBER}', style='width:1000px;height:600px;')
          else if prop != 0
            .item
              center
                  a(href='/reservation' )
                    img(src='#{ob[prop].RESERVNAME}', alt='#{ob[prop].ROOMNUMBER}', style='width:1000px;height:600px;')

      a.left.carousel-control(href='#carousel-example-generic', role='button', data-slide='prev')
            span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
            span.sr-only <
      a.right.carousel-control(href='#carousel-example-generic', role='button', data-slide='next')
            span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
            span.sr-only >


    .col-md-4.col-sm-12
      .contact-form.bottom
    script(type='text/javascript', src='js/jquery.js')
    script(type='text/javascript', src='js/bootstrap.min.js')
    script(type='text/javascript', src='js/lightbox.min.js')
    script(type='text/javascript', src='js/wow.min.js')
    script(type='text/javascript', src='js/main.js')
